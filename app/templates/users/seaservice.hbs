{{#paper-toolbar as |toolbar|}}
  {{#sticky-container
    options=stickyOptions
    class='sticky-container'
  }}
    {{#toolbar.tools
      class='toolbar-tools'
    }}
      {{ui/link-button
        destination='users.seaservice.index'
        iconButton=true
        icon="list"
        class='zoom-in-icon'
        tooltipText='List all'
      }}
      {{ui/link-button
        destination='users.seaservice.new'
        iconButton=true
        icon="add"
        class='zoom-in-rotate90-icon'
        tooltipText='Add new seaservice'
      }}
    {{/toolbar.tools}}
  {{/sticky-container}}
{{/paper-toolbar}}
{{outlet}}
