<h3>
  Last seaservice
  {{#link-to 'users.seaservice' class='zoom-in-icon'}}(go to seaservice){{/link-to}}
</h3>
<div class='tbl-header'>
  <table cellpadding="0" cellspacing="0" border="0">
    <thead>
    <tr class='header-row'>
      <th class='col-xs-3'>Vessel</th>
      <th class='col-xs-3'>Position</th>
      <th class='col-xs-3'>Sign on</th>
      <th class='col-xs-3'>Sign off</th>
    </tr>
    </thead>
  </table>
</div>
<div class='tbl-content col-xs-12'>
  <table cellpadding="0" cellspacing="0" border="0">
    <tbody>
  {{#if seaservice}}
    {{dashboard/last-seaservice-table/item seaservice=seaservice session=session}}
  {{else}}
    <p class='no-data'>
      No registered seaservice
      {{#link-to 'users.seaservice.new' class='link-item'}}
        Add new seaservice
      {{/link-to}}
    </p>
  {{/if}}
    </tbody>
  </table>
</div>
