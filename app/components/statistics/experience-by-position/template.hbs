  <h2>Experience</h2>
  <div class="tbl-header">
    <table cellpadding="0" cellspacing="0" border="0">
      <thead>
      <tr class='header-row'>
        <th>Position</th>
        <th>Experience (DP hours / days)</th>
      </tr>
      </thead>
    </table>
  </div>
  <div class="tbl-content">
    <table cellpadding="0" cellspacing="0" border="0">
      <tbody>
      {{#each experienceByPosition as |experience|}}
        {{statistics/experience-by-position/item experience=experience}}
      {{/each}}
      </tbody>
    </table>
  </div>
